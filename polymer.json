{
  "entrypoint": "index.html",
  "shell": "src/unifide-app.js",
  "fragments": [
    "src/unifide-icons.js",
    "src/unifide-landing.js",
    "src/unifide-landing-about.js",
    "src/unifide-landing-first.js",
    "src/unifide-landing-footer.js",
    "src/unifide-landing-layout-box.js",
    "src/unifide-landing-logo.js",
    "src/unifide-landing-services.js",
    "src/unifide-landing-slogan.js",
    "src/unifide-landing-social.js"
  ],
  "sources": [
   "src/**/*",
   "data/**/*",
   "images/**/*",
   "node_modules/**/*"
  ],
  "extraDependencies": [
    "node_modules/@polymer/polymer/polymer.js"
  ],
  "lint": {
    "rules": ["polymer-2-hybrid"]
